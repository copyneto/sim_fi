   <LONGTEXTS_ZFMFI_LANC_F47_RWBAPI01>
    <item>
     <DOKIL>
      <ID>FU</ID>
      <OBJECT>ZFMFI_LANC_F47_RWBAPI01</OBJECT>
      <LANGU>D</LANGU>
      <TYP>T</TYP>
      <VERSION>0012</VERSION>
      <MASTERLANG>X</MASTERLANG>
      <TXTLINES>00020</TXTLINES>
      <DOKSTATE>A</DOKSTATE>
     </DOKIL>
     <HEAD>
      <TDOBJECT>DOKU</TDOBJECT>
      <TDNAME>ZFMFI_LANC_F47_RWBAPI01</TDNAME>
      <TDID>FU</TDID>
      <TDSPRAS>D</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00012</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>DEFINE &amp;TEXT&amp;  = &apos;Rechnungswesen-BAPI Plug-In: User-Exit&apos;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>DEFINE &amp;EVENT&amp; = &apos;RWBAPI01&apos;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>DEFINE &amp;TYPE&amp;  = &apos;INTERFACE&apos;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>INCLUDE FI_OPEN_FI_DOCUMENTATION_1 OBJECT DOKU ID TX</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>EVENT</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Der Zeitpunkt wird in den Rechnungswesen-BAPIs</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>BAPI_ACC_DOCUMENT_CHECK und</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/</TDFORMAT>
       <TDLINE>BAPI_ACC_DOCUMENT_POST</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>durchlaufen, nachdem sämtliche Konvertierungen und Prüfungen im BAPI</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>stattgefunden haben und der Rechnungswesenbeleg aufgebaut ist.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/</TDFORMAT>
       <TDLINE>Die aktuellen Belegdaten können an dieser Stelle verändert bzw.</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>angereichert werden.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/</TDFORMAT>
       <TDLINE>Nach Aufruf dieses Events wird die R/3-Rechnungswesen-Schnittstelle</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>gerufen.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>SCHNITTSTELLE</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Zur Verfügung steht der Rechnungswesenbeleg mit Belegkopf und</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Belegzeilen (C_ACCHD und C_ACCIT).</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/</TDFORMAT>
       <TDLINE>Zusätzlich kann der Quellensteuerparameter (C_ACCWT) und eine</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>kundeneigenene Struktur (C_EXTENSION) versorgt werden, die beim Aufruf</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>des BAPI versorgt wird.</TDLINE>
      </TLINE>
     </LINES>
    </item>
    <item>
     <DOKIL>
      <ID>FU</ID>
