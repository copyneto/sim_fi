    <item>
     <DOKIL>
      <ID>FU</ID>
      <OBJECT>ZDMEE_EXIT_BARCOD_ITAU</OBJECT>
      <LANGU>D</LANGU>
      <TYP>T</TYP>
      <VERSION>0002</VERSION>
